# model date infomation
date_start : '01/01/1999'
date_end : '01/01/2000'
days_per_run : 10 

# forcing data flags
#   if more than once forcing type is 'T', provide
#   a separate ww3_prnc.config for each 'T' forcing, e.g.
#         ww3_prnc.config.wind
#         ww3_prnc.config.currents
#         ww3_prnc.config.ssh
#         ww3_prnc.config.ice
#         ww3_prnc.config.icebergs
wind : 'T'
currents : 'F'
ssh : 'F'
ice : 'F'
icebergs : 'F'

# data directory paths
wind_direc : '../wind_data/'
currents_direc : '../current_data/'
ssh_direc : '../ssh_data/'
ice_direc : '../ice_data/'
iceberg_direc: '../iceberg_data/'

# run information
machine : 'grizzly'
job_name : 'ww3_glo_1d'
queue : 'interactive'
exe : 'ww3_shel'
ncores : 216

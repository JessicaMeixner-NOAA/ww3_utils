# model date infomation
date_start : '01/01/1999'
date_end : '01/01/2000'
days_per_run : 90 

# forcing data flags
#   if more than once forcing type is 'T', provide
#   a separate ww3_prnc.config for each 'T' forcing, e.g.
#         ww3_prnc.config.wind
#         ww3_prnc.config.currents
#         ww3_prnc.config.ssh
#         ww3_prnc.config.ice
#         ww3_prnc.config.icebergs
wind : 'T'
currents : 'F'
ssh : 'F'
ice : 'T'
icebergs : 'F'

# data directory paths
wind_direc : '../reanalysis_data/wind/'
currents_direc : '../reanalysis_data/currents/'
ssh_direc : '../reanalysis_data/ssh/'
ice_direc : '../reanalysis_data/ice/'
iceberg_direc: '../reanalysis_data/icebergs/'

# run information
machine : 'badger'
job_name : 'ww3_glo_30m'
queue : 'standard'
exe : 'ww3_shel'
ncores : 1800
